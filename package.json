{
  "name": "idm",
  "version": "0.1.0",
  "description": "Identity Management Service",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/idm.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "build": "mkdir -p dist && webpack --config config/webpack-production-config.js",
    "icons:fetch": "mkdir -p dist && curl -s https://icons.learnersguild.org/tags > dist/icons-metadata.json",
    "db:create": "./node_modules/.bin/babel-node ./db/create.js",
    "db:drop": "./node_modules/.bin/babel-node ./db/drop.js",
    "db:migrate:configure": "node ./db/config.js > ./db/database.json",
    "db:migrate": "npm run db:migrate:configure && node ./node_modules/.bin/rethink-migrate -r db",
    "start": "npm run icons:fetch && node server",
    "postinstall": "npm run build",
    "test": "./node_modules/.bin/xo && NODE_ENV=test ./node_modules/.bin/ava"
  },
  "keywords": [
    "idm"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/idm/issues"
  },
  "homepage": "https://github.com/LearnersGuild/idm#readme",
  "dependencies": {
    "auth0-js": "^6.8.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bourbon": "^4.2.6",
    "bourbon-neat": "^1.7.2",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^2.0.4",
    "dotenv": "^1.2.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-graphql": "^0.4.9",
    "express-sslify": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "graphql": "^0.4.16",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^5.4.1",
    "lodash": "^3.10.1",
    "material-ui": "^0.14.4",
    "node-sass": "^3.4.2",
    "normalize-css": "^2.3.1",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss": "^5.0.12",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "raven": "^0.9.0",
    "raven-js": "^2.0.0-rc2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.0.5",
    "redux-form": "^4.1.10",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "resolve-url-loader": "^1.4.3",
    "rethink-migrate": "jeffreywescott/rethink-migrate",
    "rethinkdb": "^2.2.1",
    "sass-loader": "^3.1.2",
    "sass-resources-loader": "^1.0.2",
    "serve-static": "^1.10.0",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.0.6-beta",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4",
    "yamljs": "^0.2.4"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.16.1",
    "jsdom": "^8.0.2",
    "piping": "^0.3.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "xo": "^0.12.1"
  },
  "engines": {
    "node": "5.6.x"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "comma-dangle": [
        0
      ],
      "no-negated-condition": [
        0
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "quotes": [2, "single", "avoid-escape"]
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  },
  "ava": {
    "files": [
      "**/__tests__/*-test.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  }
}
