{
  "name": "idm",
  "version": "0.1.0",
  "description": "Identity Management Service",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/idm.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "build": "mkdir -p dist && webpack --config config/webpack-$NODE_ENV-config.js",
    "db:create": "./node_modules/.bin/babel-node ./db/create.js",
    "db:drop": "./node_modules/.bin/babel-node ./db/drop.js",
    "db:migrate:configure": "node ./db/config.js > ./db/database.json",
    "db:migrate": "npm run db:migrate:configure && node ./node_modules/.bin/rethink-migrate -r db",
    "lint": "./node_modules/.bin/xo",
    "start": "npm run build & node server",
    "test": "npm run lint && NODE_ENV=test ./node_modules/.bin/jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/config/jest-preprocessor.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "keywords": [
    "idm",
    "swagger"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/idm/issues"
  },
  "homepage": "https://github.com/LearnersGuild/idm#readme",
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bluebird": "^3.0.5",
    "bootstrap": "github:twbs/bootstrap#v4.0.0-alpha.2",
    "bootstrap-loader": "^1.0.7",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^2.0.4",
    "dotenv": "^1.2.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-sslify": "^1.0.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "jsonwebtoken": "^5.4.1",
    "lodash": "^3.10.1",
    "node-fetch": "^1.3.3",
    "node-sass": "^3.4.2",
    "passport": "^0.3.2",
    "passport-google-oauth": "^0.2.0",
    "postcss": "^5.0.12",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "raven": "^0.9.0",
    "raven-js": "^2.0.0-rc2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-simple-router": "^1.0.2",
    "resolve-url-loader": "^1.4.3",
    "rethink-migrate": "jeffreywescott/rethink-migrate",
    "rethinkdb": "^2.2.1",
    "sass-loader": "^3.1.2",
    "serve-static": "^1.10.0",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "swagger-tools": "0.9.*",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0",
    "yamljs": "^0.2.4"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.16.1",
    "jest-cli": "^0.8.2",
    "piping": "^0.3.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "xo": "^0.12.1"
  },
  "engines": {
    "node": "5.5.x",
    "npm": "3.3.x"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "comma-dangle": [0],
      "no-negated-condition": [0]
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  }
}
